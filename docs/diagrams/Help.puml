@startuml
actor User
participant SpendSwift
participant Parser
participant UI

User -> SpendSwift : input("help")
activate SpendSwift
SpendSwift -> Parser : parseCommand(input, trackerData)

activate Parser
Parser -> UI : printHelpMessage()

activate UI
UI --> Parser : return

deactivate UI
Parser --> SpendSwift : return

deactivate Parser

SpendSwift --> User : return
deactivate SpendSwift


@enduml